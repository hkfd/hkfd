{
  "name": "heckford-angular",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e --prod --port 4200",
    "api-init": "cd src/api && yarn && yarn build",
    "api-update": "git submodule update --remote && yarn api-init",
    "form-init": "cd src/form && composer install",
    "form-serve": "cd src/form && yarn serve",
    "form-update": "git submodule update --remote && yarn form-init",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,scss,json}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.0.7",
    "@angular/common": "6.0.7",
    "@angular/compiler": "6.0.7",
    "@angular/core": "6.0.7",
    "@angular/forms": "6.0.7",
    "@angular/http": "6.0.7",
    "@angular/platform-browser": "6.0.7",
    "@angular/platform-browser-dynamic": "6.0.7",
    "@angular/router": "6.0.7",
    "core-js": "^2.5.3",
    "intersection-observer": "^0.5.0",
    "js-cookie": "^2.2.0",
    "prismic-dom": "^2.1.0",
    "prismic-javascript": "^1.5.0-beta.4",
    "raven-js": "^3.26.3",
    "rxjs": "^6.2.1",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "^6.0.8",
    "@angular/compiler-cli": "6.0.7",
    "@angular/language-service": "6.0.7",
    "@types/google.analytics": "0.0.38",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/js-cookie": "^2.1.0",
    "@types/node": "~9.4.7",
    "codelyzer": "^4.2.1",
    "husky": "^0.14.3",
    "jasmine": "^2.99.0",
    "jasmine-core": "^2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.7",
    "protractor": "~5.3.0",
    "protractor-console": "^3.0.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "2.7.2"
  },
  "repository": "https://rajansolanki@bitbucket.org/heckford/heckford-angular.git"
}
