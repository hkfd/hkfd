{
  "name": "angular",
  "version": "1.3.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "build": "ng build --prod",
    "test": "yarn test:browser && yarn test:server && yarn e2e",
    "test:browser": "yarn jest --config=jest.browser.config.js",
    "test:browser:watch": "yarn test:browser --watch",
    "test:server": "jest server.spec.ts --config=jest.server.config.js",
    "test:server:watch": "yarn test:server --watch --verbose false",
    "lint": "ng lint",
    "e2e": "ng e2e --port 4200",
    "compile:server": "tsc -p server.tsconfig.json",
    "build:client-and-server-bundles": "yarn build -c $ENVIRONMENT && ng run angular:server -c $ENVIRONMENT",
    "build:ssr": "yarn build:client-and-server-bundles && yarn compile:server",
    "serve:ssr": "node --optimize_for_size --max_old_space_size=460 --gc_interval=100 dist/server"
  },
  "dependencies": {
    "@angular/animations": "6.1.7",
    "@angular/common": "6.1.7",
    "@angular/compiler": "6.1.7",
    "@angular/core": "6.1.7",
    "@angular/forms": "6.1.7",
    "@angular/http": "6.1.7",
    "@angular/platform-browser": "6.1.7",
    "@angular/platform-browser-dynamic": "6.1.7",
    "@angular/platform-server": "^6.1.7",
    "@angular/router": "6.1.7",
    "@nguniversal/express-engine": "^6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "api": "^1.3.0",
    "compression": "^1.7.3",
    "core-js": "^2.5.7",
    "email": "^1.2.0",
    "helmet": "^3.13.0",
    "intersection-observer": "^0.5.0",
    "memjs": "^1.2.0",
    "prismic-dom": "^2.1.0",
    "raven": "^2.6.4",
    "raven-js": "^3.26.4",
    "rxjs": "^6.3.2",
    "ts-loader": "^4.4.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular/cli": "^6.2.1",
    "@angular/compiler-cli": "6.1.7",
    "@angular/language-service": "6.1.7",
    "@types/compression": "^0.0.36",
    "@types/express": "^4.16.0",
    "@types/google.analytics": "0.0.39",
    "@types/helmet": "^0.0.40",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^23.3.1",
    "@types/js-cookie": "^2.1.0",
    "@types/memjs": "^1.2.0",
    "@types/node": "~10.5.3",
    "@types/prismic-dom": "^2.0.2",
    "@types/raven": "^2.5.1",
    "@types/supertest": "^2.0.5",
    "codelyzer": "^4.4.2",
    "jasmine": "^3.1.0",
    "jasmine-core": "^3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "23",
    "jest-preset-angular": "^6.0.2",
    "protractor": "~5.4.0",
    "protractor-console": "^3.0.0",
    "supertest": "^3.2.0",
    "ts-jest": "^23.10.5",
    "ts-node": "~7.0.0",
    "typescript": "2.9.2"
  }
}
