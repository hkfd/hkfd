{
  "name": "heckford-angular",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e --port 4200",
    "build:client-and-server-bundles": "ng build && ng run heckford-angular:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "build:ssr": "yarn build:client-and-server-bundles && yarn webpack:server",
    "serve:ssr": "DEBUG=express:* node dist/server",
    "api-init": "cd src/api && yarn && yarn build",
    "api-update": "git submodule update --remote && yarn api-init",
    "form-init": "cd src/form && composer install",
    "form-serve": "cd src/form && yarn serve",
    "form-update": "git submodule update --remote && yarn form-init",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,js,scss,md,json}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.1.0",
    "@angular/common": "6.1.0",
    "@angular/compiler": "6.1.0",
    "@angular/core": "6.1.0",
    "@angular/forms": "6.1.0",
    "@angular/http": "6.1.0",
    "@angular/platform-browser": "6.1.0",
    "@angular/platform-browser-dynamic": "6.1.0",
    "@angular/platform-server": "^6.1.0",
    "@angular/router": "6.1.0",
    "@nguniversal/express-engine": "^6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "core-js": "^2.5.7",
    "intersection-observer": "^0.5.0",
    "prismic-dom": "^2.1.0",
    "raven-js": "^3.26.4",
    "rxjs": "^6.2.2",
    "ts-loader": "^4.4.2",
    "web-animations-js": "^2.3.1",
    "webpack": "^4.16.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular/cli": "^6.1.0",
    "@angular/compiler-cli": "6.1.0",
    "@angular/language-service": "6.1.0",
    "@types/express": "^4.16.0",
    "@types/google.analytics": "0.0.39",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/js-cookie": "^2.1.0",
    "@types/node": "~10.5.3",
    "codelyzer": "^4.4.2",
    "husky": "^0.14.3",
    "jasmine": "^3.1.0",
    "jasmine-core": "^3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.5",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.2.0",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.7",
    "protractor": "~5.4.0",
    "protractor-console": "^3.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": ">=2.7.0 <2.8.0",
    "webpack-cli": "^3.1.0"
  },
  "repository": "https://rajansolanki@bitbucket.org/heckford/heckford-angular.git"
}
