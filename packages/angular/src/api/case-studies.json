  {
    "id": "lloyd-motor-group",
    "title": "LLOYD MOTOR GROUP",
    "featured": false,
    "colour": "white",
    "thumbnail": {
      "name": "lloyd-motor-group-thumb",
      "alt": "Lloyd Motor Group"
    },
    "overview": [
